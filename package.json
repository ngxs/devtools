{
  "name": "devtools",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "cp:manifest": "cp manifest.json dist/",
    "start": "concurrently \"yarn build:watch\" \"node scripts/cp-assets.js watch\"",
    "start:serve": "ng serve",
    "build:watch": "ng build --watch --progress false",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint && prettier --list-different \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.5",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ngxs/logger-plugin": "^3.1.4",
    "@ngxs/store": "^3.1.4",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "chokidar": "^2.0.3",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mkdirp": "^0.5.1",
    "prettier": "^1.13.5",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
